<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <appSettings>
    <!--
      Regular expression that is used by Fare to generate a matching string.
    -->
    <add key="pattern" value="[a-z]{5}" />
    <!-- 
      TimeSpan.Parse pattern: [ws][-]{ d | [d.]hh:mm[:ss[.ff]] }[ws] 
      https://msdn.microsoft.com/en-us/library/se73z7b9(v=vs.110).aspx
    -->
    <add key="timespan" value="00:00:01" />
  </appSettings>
</configuration>